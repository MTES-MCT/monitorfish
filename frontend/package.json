{
  "name": "monitorfish-frontend",
  "description": "Monitorfish web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "scripts": {
    "build": "npm run clean && vite build && npm run bundle-sw",
    "clean": "rm -Rf ./build ./public/service-worker.js ./node-modules/.cache ./node-modules/.vite",
    "dev": "import-meta-env-prepare -x ./.env.local.defaults && vite --port 3000",
    "bundle-sw": "esbuild src/workers/serviceWorker.ts --bundle --outfile=public/service-worker.js",
    "dev-monitorenv": "FRONTEND_MONITORENV_URL=//localhost:9880 import-meta-env-prepare -u -x ./.env.local.defaults && vite --port 3000",
    "dev-puppeteer": "FRONTEND_OIDC_ENABLED=false FRONTEND_MONITORENV_URL=//localhost:9880 import-meta-env-prepare -u -x ./.env.local.defaults && vite --port 3000",
    "prepare": "cd .. && ./frontend/node_modules/.bin/husky ./frontend/config/husky",
    "generate:perfdata": "node ./scripts/generate_perf_data.js",
    "generate:testdata": "node ./scripts/generate_test_data_seeds.js",
    "start": "import-meta-env-prepare -u -x ./.env.local.defaults && vite preview --port 3000",
    "start:emulate": "npm run build && import-meta-env -x ./.env.example -p ./build/index.html && import-meta-env-prepare -u -x ./.env.local.defaults && vite preview --port 3000",
    "test:deps": "madge --circular ./src/index.tsx",
    "test:e2e": "cypress run --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "test:e2e:flaky": "./cypress/support/test_flaky_test.sh",
    "test:e2e:generate": "./scripts/clean_wiremock_stubs.sh && node ./scripts/generate_wiremock_stubs.js",
    "test:e2e:open": "cypress open --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "test:multi-windows:open": "IS_HEADLESS=false jest --config=./config/multi-windows/jest.config.js",
    "test:multi-windows:run": "IS_HEADLESS=true jest --config=./config/multi-windows/jest.config.js",
    "test:lint": "eslint --config=./.eslintrc.cjs ./src",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test:type": "tsc",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "6.0.1",
    "@mtes-mct/monitor-ui": "24.12.1",
    "@reduxjs/toolkit": "2.5.1",
    "@sentry/react": "8.53.0",
    "@sentry/vite-plugin": "^2.22.7",
    "@tanstack/react-table": "8.21.2",
    "@tanstack/react-virtual": "3.11.3",
    "comlink": "4.4.2",
    "date-fns": "3.6.0",
    "dayjs": "1.11.13",
    "deep-object-diff": "1.1.9",
    "diacritics": "1.3.0",
    "esbuild": "0.25.0",
    "export-to-csv": "0.2.1",
    "formik": "2.4.6",
    "fuse.js": "7.0.0",
    "hammerjs": "2.0.8",
    "i18n-iso-countries": "7.13.0",
    "imask": "7.6.1",
    "ky": "1.7.4",
    "lodash": "4.17.21",
    "nouislider": "15.8.1",
    "oidc-client-ts": "2.4.1",
    "ol": "9.2.4",
    "ol-mapbox-style": "12.4.0",
    "print-js": "1.6.0",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-color": "2.19.3",
    "react-device-detect": "2.2.3",
    "react-dom": "18.3.1",
    "react-highlight-words": "0.21.0",
    "react-imask": "7.6.1",
    "react-markdown": "8.0.7",
    "react-oidc-context": "2.3.1",
    "react-redux": "9.2.0",
    "react-router-dom": "6.27.0",
    "react-select": "5.10.0",
    "react-toastify": "9.1.3",
    "redux": "5.0.1",
    "redux-mock-store": "1.5.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "3.1.0",
    "rsuite": "5.54.0",
    "rsuite-table": "5.19.1",
    "simplify-geojson": "1.0.5",
    "styled-components": "6.1.14",
    "timeago.js": "4.0.2",
    "use-debounce": "10.0.4",
    "uuid": "10.0.0",
    "worker-loader": "3.0.8",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yup": "1.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.4.0",
    "@import-meta-env/cli": "0.7.3",
    "@import-meta-env/prepare": "0.2.2",
    "@import-meta-env/unplugin": "0.6.2",
    "@jest/globals": "29.7.0",
    "@rollup/plugin-replace": "6.0.2",
    "@swc/core": "1.10.12",
    "@swc/jest": "0.2.37",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/vite-react": "2.0.1",
    "@types/diacritics": "1.3.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.0",
    "@types/ramda": "0.30.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-highlight-words": "0.20.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-react": "4.3.4",
    "assert": "2.1.0",
    "codecov": "3.8.3",
    "cypress": "13.17.0",
    "cypress-mouse-position": "1.0.0",
    "cypress-plugin-snapshots": "github:MTES-MCT/cypress-plugin-snapshots",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mocha": "10.5.0",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "eventsourcemock": "2.0.0",
    "got": "13.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "14.0.1",
    "madge": "8.0.0",
    "ora": "8.1.1",
    "postgres": "3.4.5",
    "prettier": "3.4.2",
    "puppeteer": "22.15.0",
    "strip-json-comments": "5.0.1",
    "ts-jest": "29.2.5",
    "type-fest": "4.33.0",
    "typescript": "5.7.3",
    "vite": "5.4.14",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "whatwg-fetch": "3.6.20"
  },
  "overrides": {
    "socket.io": ">=2.5.0",
    "ws": ">=7.5.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "//": "https://github.com/okonet/lint-staged/issues/825#issuecomment-674575655",
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --config=./.eslintrc.cjs --fix",
      "bash -c 'npm run test:type'"
    ]
  }
}
