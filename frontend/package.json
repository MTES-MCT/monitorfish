{
  "name": "monitorfish-frontend",
  "description": "Monitorfish web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "scripts": {
    "build": "sh -ac '. ../infra/configurations/frontend/.env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "cypress:open": "cypress open --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "cypress:run": "cypress run --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "dev": "BROWSER=none DISABLE_ESLINT_PLUGIN=true sh -ac '. ../infra/configurations/frontend/.env.local; react-scripts start'",
    "eject": "react-scripts eject",
    "prepare": "cd .. && ./frontend/node_modules/.bin/husky install ./frontend/config/husky",
    "start": "sh -ac '. ../infra/configurations/frontend/.env.local; react-scripts start'",
    "test:e2e": "cypress run --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "test:lint": "eslint ./src",
    "test:lint:partial": "eslint --config=./.eslintrc.partial.js ./src ./cypress",
    "test:lint:partial:fix": "npm run test:lint:partial -- --fix",
    "test:type": "tsc",
    "test:type:partial": "tsc --project ./tsconfig.partial.json",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/modifiers": "6.0.0",
    "@mtes-mct/monitor-ui": "8.0.0",
    "@reduxjs/toolkit": "1.9.3",
    "@sentry/browser": "7.55.2",
    "@sentry/react": "7.52.1",
    "@sentry/tracing": "7.47.0",
    "@svgr/webpack": "8.0.1",
    "comlink": "4.4.1",
    "date-fns": "2.30.0",
    "dayjs": "1.11.9",
    "diacritics": "1.3.0",
    "export-to-csv": "0.2.1",
    "formik": "2.2.9",
    "fuse.js": "6.6.2",
    "hammerjs": "2.0.8",
    "i18n-iso-countries": "7.6.0",
    "imask": "7.0.1",
    "immer": "9.0.17",
    "ky": "0.33.3",
    "lodash": "4.17.21",
    "mini.css": "3.0.1",
    "nouislider": "15.7.1",
    "oidc-client-ts": "^2.2.4",
    "ol": "7.4.0",
    "ol-mapbox-style": "10.6.0",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-coordinate-input": "1.0.0",
    "react-device-detect": "2.2.2",
    "react-dom": "18.2.0",
    "react-epic-spinners": "0.5.0",
    "react-highlight-words": "0.20.0",
    "react-imask": "6.4.3",
    "react-markdown": "8.0.7",
    "react-oidc-context": "^2.2.2",
    "react-redux": "8.1.1",
    "react-router-dom": "6.14.1",
    "react-scripts": "5.0.1",
    "react-select": "5.7.4",
    "react-toastify": "9.1.3",
    "redux": "4.2.1",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.2",
    "rsuite": "5.37.0",
    "rsuite-table": "5.10.6",
    "simplify-geojson": "1.0.5",
    "styled-components": "5.3.11",
    "timeago.js": "4.0.2",
    "use-debounce": "9.0.4",
    "worker-loader": "3.0.8",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@swc/core": "1.3.58",
    "@swc/jest": "0.2.26",
    "@testing-library/react": "13.4.0",
    "@tsconfig/create-react-app": "1.0.3",
    "@types/diacritics": "1.3.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.15.11",
    "@types/ramda": "0.29.2",
