    "eventsourcemock": "^2.0.0",
    "got": "13.0.0",
    "husky": "9.0.10",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.0",
    "puppeteer": "21.7.0",
    "ts-jest": "29.1.2",
    "type-fest": "4.0.0",
