{
  "name": "monitorfish-frontend",
  "description": "Monitorfish web application.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": "18",
    "npm": "8"
  },
  "scripts": {
    "start": "sh -ac '. ../infra/configurations/frontend/.env.local; react-scripts start'",
    "build": "sh -ac '. ../infra/configurations/frontend/.env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "cypress:open": "cypress open --browser firefox --e2e",
    "cypress:open:electron": "cypress open --browser electron --e2e",
    "eject": "react-scripts eject",
    "prepare": "cd .. && husky install ./frontend/config/husky",
    "test:e2e": "cypress run --browser firefox --e2e",
    "test:lint": "eslint ./src",
    "test:lint:partial": "eslint --config=./.eslintrc.partial.js ./src ./cypress",
    "test:lint:partial:fix": "npm run test:lint:partial -- --fix",
    "test:type": "tsc",
    "test:type:partial": "tsc --project ./tsconfig.partial.json",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "^4.0.3",
    "@dnd-kit/modifiers": "^4.0.0",
    "@reduxjs/toolkit": "1.8.2",
    "@sentry/react": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "@svgr/webpack": "^5.5.0",
    "comlink": "^4.3.0",
    "date-fns": "2.29.2",
    "dayjs": "1.11.3",
    "diacritics": "1.3.0",
    "export-to-csv": "^0.2.1",
    "fuse.js": "^6.6.2",
    "hammerjs": "^2.0.8",
    "i18n-iso-countries": "^6.4.0",
    "imask": "^6.1.0",
    "immer": "^9.0.6",
    "ky": "0.31.3",
    "lodash": "^4.17.21",
    "mini.css": "^3.0.1",
    "nouislider": "14.7.0",
    "nouislider-react": "^3.4.0",
    "ol": "7.1.0",
    "ol-mapbox-style": "6.9.0",
    "ramda": "0.28.0",
    "ramda-adjunct": "3.3.0",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-coordinate-input": "^1.0.0-rc.2",
    "react-device-detect": "2.2.2",
    "react-dom": "18.2.0",
    "react-epic-spinners": "^0.5.0",
    "react-highlight-words": "0.18.0",
    "react-imask": "^6.1.0",
    "react-markdown": "^6.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "react-select": "5.4.0",
    "react-toastify": "9.0.7",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "2.4.1",
    "rsuite": "5.18.0",
    "rsuite-table": "5.6.0",
    "simplify-geojson": "^1.0.4",
    "styled-components": "5.3.5",
    "timeago.js": "^4.0.2",
    "worker-loader": "^3.0.6"
  },
  "devDependencies": {
    "@ivangabriele/eslint-config-typescript-react": "4.3.0",
    "@jest/globals": "28.1.3",
    "@swc/core": "1.2.207",
    "@swc/jest": "0.2.21",
    "@testing-library/react": "13.4.0",
    "@tsconfig/create-react-app": "1.0.2",
    "@types/diacritics": "1.3.1",
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "@types/ramda": "0.28.15",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.9",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.0",
    "babel-eslint": "10.1.0",
    "codecov": "3.8.3",
    "cypress": "10.8.0",
    "cypress-mouse-position": "1.0.0",
    "cypress-plugin-snapshots": "github:ivangabriele/cypress-plugin-snapshots",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.5",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react-testing-library": "8.0.1",
    "type-fest": "2.16.0",
    "typescript": "4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "//": "https://github.com/okonet/lint-staged/issues/825#issuecomment-674575655",
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint",
      "bash -c 'npm run test:type:partial'"
    ]
  },
  "proxy": "http://localhost:8880"
}
