    "i18n-iso-countries": "7.6.0",
    "imask": "7.1.3",
    "ky": "1.2.0",
    "lodash": "4.17.21",
    "nouislider": "15.7.1",
    "oidc-client-ts": "^2.4.0",
    "ol": "9.0.0",
    "ol-mapbox-style": "12.2.0",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-coordinate-input": "1.0.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-epic-spinners": "0.5.0",
    "react-highlight-words": "0.20.0",
    "react-imask": "6.4.3",
    "react-markdown": "8.0.7",
    "react-oidc-context": "^2.3.1",
    "react-redux": "8.1.2",
    "react-router-dom": "6.16.0",
    "react-select": "5.7.4",
    "react-toastify": "9.1.3",
    "redux": "4.2.1",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.2",
    "rsuite": "5.45.0",
    "rsuite-table": "5.12.0",
    "simplify-geojson": "1.0.5",
    "styled-components": "5.3.11",
    "timeago.js": "4.0.2",
    "use-debounce": "9.0.4",
    "uuid": "^9.0.1",
    "worker-loader": "3.0.8",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@import-meta-env/cli": "^0.6.8",
    "@import-meta-env/prepare": "0.1.13",
    "@import-meta-env/unplugin": "^0.5.1",
    "@jest/globals": "29.7.0",
    "@swc/core": "1.3.102",
    "@swc/jest": "0.2.29",
    "@testing-library/react": "14.2.0",
