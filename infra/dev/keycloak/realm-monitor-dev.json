      "defaultClientScopes": ["openid", "profile", "email", "siret"],
      "optionalClientScopes": []
    }
  ],
  "clientScopes": [
    {
      "name": "openid",
      "description": "OpenID Connect built-in scope: openid",
      "protocol": "openid-connect",
      "attributes": {
        "consent.screen.text": "OpenID Connect",
        "display.on.consent.screen": "true"
      },
      "protocolMappers": [
        {
          "name": "sub",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-sub-mapper",
          "consentRequired": false,
          "config": {}
        }
      ]
    },
    {
      "name": "email",
      "description": "OpenID Connect built-in scope: email",
      "protocol": "openid-connect",
      "attributes": {
        "consent.screen.text": "Email address",
        "display.on.consent.screen": "true"
      },
      "protocolMappers": [
        {
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "email",
            "jsonType.label": "string",
            "user.attribute": "email"
          }
        },
        {
          "name": "email verified",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "email_verified",
            "jsonType.label": "boolean",
            "user.attribute": "emailVerified"
          }
        }
      ]
    },
    {
      "name": "siret",
      "description": "SIRET scope for accessing SIRET information",
      "protocol": "openid-connect",
      "attributes": {
        "consent.screen.text": "Access to SIRET information",
        "display.on.consent.screen": "true"
      },
      "protocolMappers": [
        {
          "name": "siret-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "SIRET",
            "jsonType.label": "json",
            "user.attribute": "siret"
          }
        }
      ]
    }
  ],
