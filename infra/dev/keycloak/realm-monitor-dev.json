  "clients": [
    {
      "clientId": "monitorfish",
      "enabled": true,
      "protocol": "openid-connect",
      "redirectUris": [
        "http://localhost:8880/login/oauth2/code/proconnect",
        "http://localhost:8880",
        "http://localhost:3000"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://localhost:8880"
      ],
      "publicClient": false,
